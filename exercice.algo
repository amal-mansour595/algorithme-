ALGORITHM AnalyzeSentence
VAR
 ch:=STRING
 nb_word:=INTEGER
 nb_voyelle:=INTEGER
BEGIN
   write ("entrer a syntaxe")
   read(ch)
   nb_word:=1
   nb_voyelle:=0
   FOR (i:=0;i<ch.length;i++ )   DO
    if(ch[i]=="")
    nb_word=nb_word+1
    if (ch[i]in ["a","e","i","o","u","y","A","E","I","O","U","Y" ])
    nb_voyelle:=nb_voyelle+1
   END_FOR
   write(nb_word)
   write(nb_voyelle)
   write(ch.length)
END
